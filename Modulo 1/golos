#include <iostream>
using namespace std;

int main(){
    int jogo = 5;
    int golos[jogo];
    int totalgolos = 0;
    for(int i = 0; i < jogo; i++){
        cout << "Digite o numero de golos no " << i + 1 << " jogo: ";
        cin >> golos[i]; 
        totalgolos += golos[i];       
    }
    cout << "O total de golos marcados em" << jogo << "jogo: " << totalgolos << endl;

}
